var inlineAdChoices=document.querySelector(".exp-ac--toggle-click-17-700 .ac--inline"),tooltip=document.querySelector(".ac__container"),semaphoreAdChoice=tooltip.querySelector(".ac__link.ac__link-semaphore");
null!==inlineAdChoices&&null!==tooltip&&function(){var a=!1;inlineAdChoices.addEventListener("click",function(b){null!=semaphoreAdChoice&&1!=inlineAdChoices.semaphoreAttached&&"function"===typeof reportAd&&function(){inlineAdChoices.semaphoreAttached=!0;var a=tooltip.querySelector(".ac__link.ac__link-non-semaphore");tooltip.removeChild(a);semaphoreAdChoice.classList.toggle("ac__link-semaphore");var b=semaphoreAdChoice.dataset.account,c=semaphoreAdChoice.dataset.creative;tooltip.addEventListener("click",
function(a){a.preventDefault();reportAd("SAS_ADS_CREATIVE",c,b)})}();inlineAdChoices.classList.toggle("ac--inline--active");a?inlineAdChoices.classList.remove("ac--inline--open-left"):(b.preventDefault(),tooltip.clientWidth>getWindowWidth()-getPositionX(inlineAdChoices)&&inlineAdChoices.classList.add("ac--inline--open-left"));a=!a})}();function getWindowWidth(){return window.innerWidth||document.documentElement.clientWidth||document.body.clientWidth}
function getPositionX(a){for(var b=0;a;)b=a===document.body?b+(a.offsetLeft-(a.scrollLeft||document.documentElement.scrollLeft)+a.clientLeft):b+(a.offsetLeft-a.scrollLeft+a.clientLeft),a=a.offsetParent;return b};