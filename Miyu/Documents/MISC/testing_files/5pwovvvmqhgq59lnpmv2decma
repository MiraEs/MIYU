e.JOBS_EXT_EXTERNAL_ROUTES=s
var n=t.JOBS_SERVICES.concat(["shared-ext@messaging-overlay"])
e.JOBS_EXT_SERVICES=n})
define("jobs-ext/utils/focus-on-next-job-card",["exports","ember"],function(e,t){function s(e,s,n){var o=s.indexOf(e)+1
t.default.assert("current card should be in cardlist",0!==o)
var l=o<s.length,a=l?o+1:o,i=n.querySelector(".job-card:nth-child("+a+") a")
i&&i.focus()}e.default=s})

