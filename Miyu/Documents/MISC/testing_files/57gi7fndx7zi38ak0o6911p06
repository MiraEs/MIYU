var dropdownAdChoices=document.querySelector(".exp-ac--toggle-click .ac__container"),semaphoreAdChoice=dropdownAdChoices.querySelector(".ac__link.ac__link-semaphore"),adsContainer=document.getElementById("ads-container"),semaphoreHeaderText=document.querySelector(".ta__header-text.ta__header-semaphore-text"),originalHeaderText=document.querySelector(".ta__header-text.ta__header-nonsemaphore-text"),acContainer=document.querySelector(".ac__container"),headerContainer=document.querySelector(".ta__header"),
creativeSections=document.getElementsByClassName("ta-creative"),creativeSelectionOverlays=document.getElementsByClassName("ta-creative__semaphore-selection");
function toggleSemaphoreSelection(){adsContainer.classList.toggle("ads-container--semaphore-enabled");semaphoreHeaderText.classList.toggle("ta__header-text--inactive");originalHeaderText.classList.toggle("ta__header-text--inactive");acContainer.classList.toggle("ac__container--inactive");headerContainer.classList.toggle("ta__header--semaphore");for(var a=0;a<creativeSections.length;a++)creativeSections.item(a).classList.toggle("ta-creative--semaphore");for(a=0;a<creativeSelectionOverlays.length;a++)creativeSelectionOverlays.item(a).classList.toggle("ta-creative__semaphore-selection--active")}
null!==dropdownAdChoices&&(dropdownAdChoices.addEventListener("click",function(){if(null!=semaphoreAdChoice&&1!=dropdownAdChoices.semaphoreAttached&&"function"===typeof reportAd){dropdownAdChoices.semaphoreAttached=!0;var a=dropdownAdChoices.querySelector(".ac__link.ac__link-non-semaphore");dropdownAdChoices.querySelector(".ac__label-wrapper").removeChild(a);semaphoreAdChoice.classList.toggle("ac__link-semaphore");for(var a=function(){var a=creativeSelectionOverlays.item(b);a.addEventListener("click",
function(b){b.preventDefault();reportAd("SAS_ADS_CREATIVE",a.dataset.creative,a.dataset.account);toggleSemaphoreSelection()})},b=0;b<creativeSelectionOverlays.length;b++)a();semaphoreAdChoice.addEventListener("click",function(a){a.preventDefault();toggleSemaphoreSelection()})}dropdownAdChoices.classList.toggle("ac__container--active")}),dropdownAdChoices.addEventListener("mouseleave",function(){dropdownAdChoices.classList.remove("ac__container--active")}));